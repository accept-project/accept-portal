@model AcceptPortal.Models.Evaluation.EvaluationProject

@{
    ViewBag.Title = @AcceptPortal.Resources.Global.EvaluationDeleteProject;
}

<h2>@AcceptPortal.Resources.Global.EvaluationDeleteProject</h2>

<script src="@Url.Content("~/Scripts/jquery.validate.min.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/jquery.validate.unobtrusive.min.js")" type="text/javascript"></script>

    <table border="0">
    <tr>
    <td><a href="@Url.Action("Project", "Evaluation", new { Id =  @Model.ID })">Project</a></td>
    <td><a href="@Url.Action("Questions", "Evaluation", new { Id =  @Model.ID })">@AcceptPortal.Resources.Global.EvaluationQuestions</a></td>
    <td><a href="@Url.Action("Metrics", "Evaluation", new { Id =  @Model.ID })">@AcceptPortal.Resources.Global.EvaluationMetrics</a></td>
    @*<td><a href="@Url.Action("ViewEvaluationData", "Evaluation", new { Id =  @Model.ID })">@AcceptPortal.Resources.Global.EvaluationViewData</a></td>*@
    <td><a href="@Url.Action("ViewEvaluationData", "Evaluation", new { Id = @Model.ID, @token = @Model.AdminToken })">@AcceptPortal.Resources.Global.EvaluationViewData</a></td>
    </tr>
    </table>
    <br />

<h3>@AcceptPortal.Resources.Global.EvaluationDeleteMessageAll</h3>

    <table border="1">
    <tr>
        <td width="200"><b>@AcceptPortal.Resources.Global.EvaluationProjectID:</b></td>
        <td width="200">@Model.ID</td>
    </tr>
    <tr>
        <td width="200"><b>@AcceptPortal.Resources.Global.EvaluationName:</b></td>
        <td width="200">@Model.Name</td>
    </tr>
    <tr>
        <td><b>@AcceptPortal.Resources.Global.EvaluationOrganisation:</b></td>
        <td>@Model.Organization</td>
    </tr>
    <tr>
        <td><b>@AcceptPortal.Resources.Global.EvaluationDescription:</b></td>
        <td>@Model.Description</td>
    </tr>
    <tr>
        <td><b>@AcceptPortal.Resources.Global.EvaluationAPIKey:</b></td>
        <td>@Model.ApiKey</td>
    </tr>
    <tr>
        <td><b>@AcceptPortal.Resources.Global.EvaluationAPIKeyDomain:</b></td>
        <td>@Model.Domain</td>
    </tr>
    <tr>
        <td colspan="2">
        @using (Html.BeginForm())
        {
              <input type="submit" value="@AcceptPortal.Resources.Global.EvaluationConfirm" /> <text>|</text> @Html.ActionLink(AcceptPortal.Resources.Global.EvaluationCancel, "Project", new { id = @Model.ID })
        }
        </td>
    </tr>
    
    </table>
 